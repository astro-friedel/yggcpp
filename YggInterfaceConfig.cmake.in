include(CMakeFindDependencyMacro)
set(YGG_INSTALL_DEPS "@YGG_INSTALL_DEPS@")
foreach(dep IN LISTS YGG_INSTALL_DEPS)
  if (dep STREQUAL "Python3")
    find_dependency(${dep} COMPONENTS Interpreter Development NumPy)
  elseif (dep STREQUAL "ZeroMQ")
    include(SearchTools)
    find_package_zmq()
  else()
    find_dependency(${dep})
  endif()
endforeach()
include(${CMAKE_CURRENT_LIST_DIR}/YggInterfaceTargets.cmake)
if (TARGET YggInterface::YggInterface)
  add_library(YggInterface::CXX ALIAS YggInterface::YggInterface)
  add_library(YggInterface::C ALIAS YggInterface::YggInterface)
endif()
if (TARGET YggInterface::YggInterface_fortran)
  add_library(YggInterface::Fortran ALIAS
              YggInterface::YggInterface_fortran)
endif()